{
  "cloud-object-storage": {
	"url": {
	  "searchPatterns": [
		"cloudfoundry:$['cloud-object-storage'][0].credentials.url",
		"env:service_cloud-object-storage",
		"env:cloud-object-storage_url",
		"file:/server/localdev-config.json:$.cloud-object-storage_url"
	  ]
	},
	"username": {
	  "searchPatterns": [
		"cloudfoundry:$['cloud-object-storage'][0].credentials.username",
		"env:service_cloud-object-storage",
		"env:watson_cloud-object-storage_username",
		"file:/server/localdev-config.json:$.watson_cloud-object-storage_username"
	  ]
	},
	"password": {
	  "searchPatterns": [
		"cloudfoundry:$['cloud-object-storage'][0].credentials.password",
		"env:service_cloud-object-storage",
		"env:cloud-object-storage_password",
		"file:/server/localdev-config.json:$.cloud-object-storage_password"
	  ]
	},
	"credentials": {
	  "searchPatterns": [
		"cloudfoundry:cloud-object-storage",
		"env:cloud-object-storage_credentials",
		"file:/server/localdev-config.json:cloud-object-storage_credentials"
	  ]
	}
  }
}
